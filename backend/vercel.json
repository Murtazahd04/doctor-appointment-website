{
  "version": 2,
  "functions": {
    "backend/api/server.js": {
      "runtime": "@vercel/node@latest",
      "memory": 1024,
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "frontend/$1"
    }
  ]
}